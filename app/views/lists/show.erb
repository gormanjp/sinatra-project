<h3><%= @list.name %></h3>


<p><% @list.items.each do |item|%>
	<%= item.content %>
	<% item.frequency.times do %>
		<input type="checkbox">
	<% end %>

	<br>
<% end %></p>

<a href="/lists/<%=@list.id%>/edit">Edit List</a><br>
